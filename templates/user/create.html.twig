{% extends 'base.html.twig' %}

{% block title %}Création d'utilisateur{% endblock %}

{% block body_class %}page{% endblock %}
{% block body_id %}{% endblock %}


{% block body %}
    <div class="retour-btn"><a href="{{ path('user_list') }}">Retour</a></div>
    <section class="container">
        <h1>Création d'utilisateur</h1>
        {{ form_start(form, {'action' : path('user_create')}) }}
        {{ form_widget(form) }}

        <button type="submit" class="btn btn-success pull-right">Ajouter</button>
        {{ form_end(form) }}
    </section>
{% endblock %}